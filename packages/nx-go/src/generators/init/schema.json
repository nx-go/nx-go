{
  "$schema": "https://json-schema.org/schema",
  "$id": "@nx-go/nx-go:init",
  "title": "Go init generator options",
  "type": "object",
  "properties": {
    "skipFormat": {
      "type": "boolean",
      "description": "Skip formatting files.",
      "default": false,
      "x-priority": "internal"
    },
    "skipGoWorkspace": {
      "type": "boolean",
      "description": "Do not use Go multi-modules workspace",
      "default": false
    }
  }
}
